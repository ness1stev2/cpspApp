{
  "name": "cpsp-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "backend": "json-server --watch data/db.json",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run cpspApp:serve-ssr",
    "serve:ssr": "node dist/cpspApp/server/main.js",
    "build:ssr": "ng build && ng run cpspApp:server",
    "prerender": "ng run cpspApp:prerender",
    "images": "npm run images:resize && npm run images:compress",
    "images:resize": "node scripts/resize.mjs",
    "images:compress": "node scripts/compress.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.2",
    "date-fns": "^2.30.0",
    "express": "^4.15.2",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^8.0.0",
    "rxjs": "~7.8.0",
    "scrollreveal": "^4.0.9",
    "sharp": "^0.32.6",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.0.0",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "imagemin-jpegtran": "^7.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "schematics-scss-migrate": "^2.3.17",
    "typescript": "~5.0.2"
  }
}
